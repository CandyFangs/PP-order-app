<%= form_for(@product) do |f| %>
	<div class="form-group">
		<%= f.label :Typ %>
		<%= f.text_field :kind %>
	</div>
	<div class="form-group">
		<%= f.label :Model %>
		<%= f.text_field :model %>
	</div>
	<div class="form-group">
		<%= f.label :Opis %>
		<%= f.text_area :description %>
	</div>
	<div class="form-group">
		<%= f.label :"Cena w zł" %>
		<%= f.text_field :price %>
	</div>
	<div class="form-group">
		<%= f.label :Zdjęcie %>
		<%= f.text_field :pic %>
	</div>
<br />
<%= f.submit "Zapisz" %>
<% end %>
