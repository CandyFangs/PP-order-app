<%= form_for(@product) do |f| %>
	<div class="form-group">
		<%= f.label :kind %>
		<%= f.text_field :kind %>
	</div>
	<div class="form-group">
		<%= f.label :model %>
		<%= f.text_field :model %>
	</div>
	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_area :description %>
	</div>
	<div class="form-group">
		<%= f.label :price %>
		<%= f.text_field :price %>
	</div>
	<div class="form-group">
		<%= f.label :pic %>
		<%= f.text_field :pic %>
	</div>
<br />
<%= f.submit "Zapisz" %>
<% end %>
